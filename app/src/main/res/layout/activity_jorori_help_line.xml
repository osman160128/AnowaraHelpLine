<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="national_helpline"
            type="com.example.anowarahelpline.jororihelpline.JororiHelpLine" />
    </data>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".jororihelpline.JororiHelpLine"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <androidx.cardview.widget.CardView
                android:id="@+id/pollibidot"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardCornerRadius="6dp"
                android:elevation="6dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_margin="10dp">

                    <com.airbnb.lottie.LottieAnimationView
                        android:onClick="@{()->national_helpline.anowaraPollibittoth()}"
                        android:id="@+id/anowawrapallibidyuatImg"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:lottie_url="https://assets3.lottiefiles.com/private_files/lf30_vpbcbwwm.json"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        android:transitionName="lottieBloodActivityAnimation"
                        android:layout_alignParentRight="true"
                        />
                    <TextView
                        android:id="@+id/anowarapallibidyuat"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="আনোয়ারা পল্লী বিদ্যুৎ"
                        android:textStyle="bold"
                        android:textSize="25dp"


                        />
                    <TextView
                        android:id="@+id/anowarapallibidyuatPhone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="01769-400102"
                        android:textSize="20sp"
                        android:layout_below="@+id/anowarapallibidyuat"
                        />

                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardCornerRadius="6dp"
                android:elevation="6dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_margin="10dp">


                    <com.airbnb.lottie.LottieAnimationView
                        android:onClick="@{()->national_helpline.anowaraPolice()}"
                        android:id="@+id/anowawra_police_Img"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:lottie_url="https://assets3.lottiefiles.com/private_files/lf30_vpbcbwwm.json"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        android:transitionName="lottieBloodActivityAnimation"
                        android:layout_alignParentRight="true"
                        />
                    <TextView
                        android:id="@+id/anowara_police"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="আনোয়ার পুলিশ"
                        android:textStyle="bold"
                        android:textSize="25dp"

                        />
                    <TextView
                        android:id="@+id/anowara_police_Phone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="01320107834"
                        android:textSize="20sp"
                        android:layout_below="@+id/anowara_police"

                        />

                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/fireservice"
